{
    "name": "devflow/devflow-pro",
    "type": "project",
    "description": "DevFlow Pro - Advanced Deployment Management System",
    "keywords": ["laravel", "deployment", "docker", "ssl", "monitoring"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php": "^3.305",
        "doctrine/dbal": "^3.8",
        "google/cloud-storage": "^1.41",
        "guzzlehttp/guzzle": "^7.8",
        "laravel/framework": "^12.0",
        "laravel/reverb": "^1.6",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "league/commonmark": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.28",
        "league/flysystem-ftp": "^3.28",
        "league/flysystem-sftp-v3": "^3.28",
        "livewire/livewire": "^3.5",
        "phpseclib/phpseclib": "^3.0",
        "predis/predis": "^2.2",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-permission": "^6.0",
        "symfony/http-foundation": "^7.3.7",
        "symfony/process": "^7.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel/boost": "^1.8",
        "laravel/dusk": "^8.3",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.29",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "test:coverage-clover": "phpunit --coverage-clover coverage.xml",
        "analyse": "phpstan analyse --memory-limit=2G",
        "analyse:baseline": "phpstan analyse --memory-limit=2G --generate-baseline",
        "lint": "pint --test",
        "lint:fix": "pint",
        "ci": [
            "@lint",
            "@analyse",
            "@test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
